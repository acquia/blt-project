{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "acquia/blt": "8.x-dev"
    },
    "require-dev": {},
    "scripts": {
      "post-create-project-cmd": [
        "rm -f .travis.yml",
        "rm -f README.md",
        "rm -f LICENSE.txt",
        "blt create-project"
      ]
    }
}
